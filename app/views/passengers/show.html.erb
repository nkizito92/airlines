<h1>
  Passenger: <%= @passenger.full_name %>
</h1> <br>
<h2>Age: <%= @passenger.age %> </h2> <br>

<% if !@passenger.tickets.empty? %>
<h2> <%= @passenger.full_name %>'s Plane Tickets:</h2>
    <% @passenger.tickets.each do |tic| %>
        <%= tic.ticket_num %>
        <%= tic.price %>
    <% end %>
<%else %>
    <h2> <%= @passenger.full_name %> has no Plane tickets:</h2> <br>
<% end %>

<%= link_to "Airline List", root_path %>