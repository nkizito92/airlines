<%= form_for :user, class: "create" do |f| %>
    <%= f.label :username %> <br>
    <%= f.text_field :username %> <br>
    <%= f.label :password %> <br>
    <%= f.password_field :password %> <br>
    <%= f.label :password_confirmation %> <br>
    <%= f.password_field :password_confirmation %> <br />
    <%= f.submit 'Signup' %>
<% end %>
<p>Or try <%= link_to 'Facebook Signin', '/auth/facebook'%>.</p>