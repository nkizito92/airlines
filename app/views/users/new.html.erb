<%= form_for :user, class: "create" do |f| %>
    <%= f.label :username %> <br>
    <%= f.text_field :username %> <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
     <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %> <br />
    <%= f.submit 'Signup' %>
<% end %>
